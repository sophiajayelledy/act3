#include<iostream>
using namespace std;


int main()
{
	
	//declarations
	int month, day, year;
	
	
	
	//description
	cout<<"Enter month: ";
	cin>>month;
	cout<<"Enter day: ";
	cin>>day;
	cout<<"Enter year: ";
	cin>>year;
	
	
	//switch
		switch (month)
	{
	    case 1:
	    	if(day>31 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"January "<<day<<", "<<year;		
	        break;
	    case 2:
	    	if(day>28 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!!";
			}
			cout<<"February "<<day<<", "<<year;		
	        break;
	        
	    case 3:
	    	if(day>31 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"March "<<day<<", "<<year;		
	        break;
	    case 4:
	    	if(day>30 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"April "<<day<<", "<<year;		
	        break;
	        
	    case 5:
	    	if(day>31|| day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"May "<<day<<", "<<year;		
	        break;
		case 6:
	    	if(day>30 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"June "<<day<<", "<<year;		
	        break;
	        
		case 7:
	    	if(day>31 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"July "<<day<<", "<<year;		
	        break;
	        
		case 8:
	    	if(day>31 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"August "<<day<<", "<<year;		
	        break;
	        
	    case 9:
	    	if(day>31 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"September "<<day<<", "<<year;		
	        break;
	    
	    case 10:
	    	if(day>31 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!";
			}
			cout<<"October "<<day<<", "<<year;		
	        break;
	    case 11:
	    	if(day>30 || day<1)
	    	{
	    		cout<<"Invalid date!";
			}
			cout<<"November "<<day<<", "<<year;		
	        break;
	        
		case 12:
	    	if(day>31 || day<1)
	    	{
	    		cout<<"You have entered an invalid date!!";
			}
			cout<<"December "<<day<<", "<<year;		
	        break;
	        
	    default: cout<<"You have entered an invalid date!";
	}
	
	return 0;
}
